@model IEnumerable<SHARKNA.Models.tblUsers>

<h2>Users List</h2>

@if (Model == null || !Model.Any())
{
    <p>No users found.</p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>UserName</th>
                <th>Email</th>
                <th>Mobile Number</th>
                <th>Full Name (AR)</th>
                <th>Full Name (EN)</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                    <td>@user.MobileNumber</td>
                    <td>@user.FullNameAr</td>
                    <td>@user.FullNameEn</td>
                </tr>
            }
        </tbody>
    </table>
}